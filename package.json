{
	"name": "night-owl-x-one-dark",
	"publisher": "reda-salmi",
	"displayName": "Night Owl X One Dark",
	"description": "A dark theme consisting of a combination of the Night Owl and One Dark Pro Darker themes",
	"version": "1.0.1",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/redasalmi/night-owl-x-one-dark.git"
	},
	"author": "Reda Salmi <reda.salmi.elt@gmail.com> (https://redacodes.com/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/redasalmi/night-owl-x-one-dark/issues"
	},
	"homepage": "https://github.com/redasalmi/night-owl-x-one-dark#readme",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"label": "Night Owl X One Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/night-owl-x-one-dark.json"
			}
		]
	},
	"icon": "icon.png",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "bun run index.ts",
		"package": "bun x rimraf ./vsix && bun x mkdirp ./vsix && bun run vsce package --out ./vsix/night-owl-x-one-dark.vsix",
		"publish": "bun run vsce publish"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@vscode/vsce": "^3.7.1",
		"prettier": "^3.7.4"
	},
	"peerDependencies": {
		"typescript": "^5"
	}
}
